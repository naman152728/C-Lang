//Whether Triangle is Equilateral, Scalene or Isosceles

#include<stdio.h>

int main () {
    
    int a, b, c;

    printf("Enter the sides for a, b, c:\n");
    scanf("%d %d %d", &a, &b, &c);

    if(a == b && b == c && c == a)
    {
        printf("The Triangle is Equilateral Triangle");
    }
    else if ( (a == b || b == c || c == a) )
    {
        printf("The Triangle is Isosceles Triangle");
    }
    else if(a != b && b != c && c != a)
    {
        printf("The Triangle is Scalene Triangle");
    }


    return 0;
}
