#include<stdio.h>

int main () {

    char ch;
    
    printf("Enter the character:\n");
    scanf("%c", &ch);

    if (ch >= 97 && ch <= 122)
    {
        printf("%c is the Lowercase Character", ch);
    }
    else if (ch >= 65 && ch <= 90)
    {
        printf("%c is not Lowercase, but an Uppercase Character", ch);
    }

    return 0;
}
