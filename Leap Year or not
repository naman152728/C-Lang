#include<stdio.h>

int main () {
    
int year;;

printf("Enter the year:\n");
scanf("%d", &year);

    if (year % 400 == 0)
 {
    printf("%d is the Leap Year", year);
 }
    else if (year % 100 == 0)
 {
    printf("%d is the Leap Year", year);
 }
    else if (year % 4 == 0)
 {
    printf("%d is the Leap Year", year);
 }
    else 
 {
    printf("%d is not the Leap Year", year);
}
  
    return 0;
}
