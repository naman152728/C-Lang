//For Loop - Sum of n Natural No.

#include<stdio.h>

int main () {
    
    int num, sum = 0;

    printf("Enter the value for Num: ");
    scanf("%d", &num);

    for(int i = 1; i <= num; i++)
    {
        printf("%d\n", i);

        sum += i;
    }
    printf("The sum of the Num's provided above is %d", sum);

    return 0;
}
