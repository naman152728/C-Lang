//Write a Program to Print all Prime No. between 1 to n

#include<stdio.h>

int main () {
    
    int num, i, j;
    // int c = 0;
    printf("Enter the value for Num: ");
    scanf("%d", &num);

    for(i = 2; i <= num; i++)
    {
        int c = 0;
        for (j = 1; j <= i; j++)
        {
            if (i % j == 0)
            {
                c++;
            }
            
        }
        if(c == 2)
            {
                printf("%d\t", i);
            }
    }

    return 0;
}
