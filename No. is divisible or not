//No. is divisible or not

#include<stdio.h>

int main () {
    
    int a, b;

     printf("Enter value for a\n");
     scanf("%d", &a);

     if (a % 97 == 0)
     {
         printf("%d is divisible by 97", a);
     }
     else if (b = a % 97)
     {
         printf("%d is not divisible by 97 and the remainder is %d", a, b);
     }

    return 0;
}
