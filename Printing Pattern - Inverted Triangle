// Write a program to Print a Inverted Triangle

#include<stdio.h>

int main () {
    
    int num, row, col;

    printf("Enter the value for Num: ");
    scanf("%d", &num);

    for(row = 1; row <= num; row++)
    {
        col = 1;
        while(col <= (num - row))
        {
            printf(" ");
            col++;
        }
        while(col <= num)
        {
            printf("*");
            col++;
        }
        printf("\n");
    }

    return 0;
}

